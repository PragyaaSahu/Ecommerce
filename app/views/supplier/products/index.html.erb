 <h1 style="color: orange; text-align: center; text-underline-position: right;">Products</h1>
 <table class="table table-bordered">
  <thead class="thead-dark">
    <tr>
      <th>Name</th>
      <th>Category</th>
      <th>Action</th>
      
    </tr>
  </thead>
 

  <% @products.each do |product| %>
    <tbody>
      <tr>
        <td><%= product.name %></td>
        <td><%= product.category.name%>
        <td><%= link_to 'show', supplier_product_path(product) %>
      </tr>
    </tbody>
  <% end %>
 </table>
 <center>
<%= link_to 'Add Product', supplier_categories_path, class:"btn btn-sm",style:"background-color: #122b48; margin-bottom: 10px; color: white;margin-right: 10px"%>
</center>