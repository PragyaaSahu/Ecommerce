
<h3> Orders </h3>
<table class="table table-bordered table-striped">
  <thead class="thead-dark">
     <tr>
       <th>Name</th>
       <th>Email</th>
       <th>Address</th>
       <th>Details</th>
      
     </tr>
 </thead>
 <tbody>
  <% @orders.each do |order|%>
    <tr>  
	  <td><%=order.name %></td>
      <td><%= order.email%></td>
	  <td><%= order.address%></td>
      <td><%= link_to 'Show', customer_order_path(id: order.id)%></td>
	</tr>
  <%end%>
 </tbody>
</table>